ground-repeat: no-repeat;\n    }\n    \n    .globalHeader {\n        /* border: 1px solid red; */\n        margin: 0 20px 0px 20px;\n        padding: 0 auto;\n    }\n    \n    .time {\n        font-weight: 200;\n        line-height: 56px;\n        font-size: 56px;\n        color: white;\n        margin: 40px auto 20px auto;\n        text-align: center;\n    }\n    \n    .searchForm {\n        position: relative;\n        /* margin: 0 20px 22px 20px; */\n        justify-content: space-between;\n    }\n    \n    @media(min-width:500px) {\n        .globalHeader {\n            /* border: 1px solid red; */\n            margin: 120px 20px 100px 20px;\n            padding: 0 auto;\n        }\n        .searchForm {\n            display: flex;\n            justify-content: space-between;\n            max-width: 550px;\n            margin: 0 auto;\n        }\n        .globalMain {\n            max-width: 1000px;\n            margin-left: auto;\n            margin-right: auto;\n        }\n    }\n    \n    .searchForm>input {\n        /* text-align: center; */\n        width: 100%;\n        outline: none;\n        height: 40px;\n        text-indent: 10px;\n        border: 1px solid #dddddd;\n        border-radius: 20px;\n    }\n    \n    .searchForm>button {\n        position: absolute;\n        right: 0;\n        top: 4px;\n        width: 60px;\n        height: 80%;\n        background-color: white;\n        border: none;\n        color: white;\n        border-radius: 20px;\n    }\n    \n    @media(min-width:500px) {\n        .searchForm {\n            width: 180px;\n            margin-top: 50px;\n            transition: all 0.8s;\n        }\n        .searchForm:hover {\n            width: 450px;\n        }\n        .searchForm:focus-within {\n            width: 450px;\n        }\n    }\n    \n    .searchForm>button svg {\n        color: slategray;\n        width: 30px;\n        height: 30px;\n    }\n    \n    .siteList {\n        /* border: 1px solid red; */\n        margin: 